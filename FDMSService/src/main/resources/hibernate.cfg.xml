<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration SYSTEM 
"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">

<hibernate-configuration>
	<session-factory>
		<property name="hibernate.dialect">
			org.hibernate.dialect.MySQLDialect
		</property>
		<property name="hibernate.connection.driver_class">
			com.mysql.jdbc.Driver
		</property>
		<property name="hibernate.connection.url">
			jdbc:mysql://localhost:3306/fdmanagementsystem
		</property>
		<property name="hibernate.connection.username">
			root
		</property>
		<property name="hibernate.connection.password">
			Monday123
		</property>
		<mapping class="com.slickcode.fdms.service.domain.BankDO"></mapping>
		<mapping class="com.slickcode.fdms.service.domain.PersonDO"></mapping>
		<mapping class="com.slickcode.fdms.service.domain.FdDO"></mapping>
		<mapping class="com.slickcode.fdms.service.domain.LoginDO"></mapping>
		<mapping class="com.slickcode.fdms.service.domain.SecurityQuestionDO"></mapping>
	</session-factory>
</hibernate-configuration>